public class Main {
    public static void main(String[] args) {
        int num=3;
        int element=100;
        int []arr={1,2,3,4,5,0};
        for (int i=arr.length-1;i>num;i--){
            arr[i]=arr[i-1];
        }
        arr[num]=element;
        for (int i=0;i<arr.length;i++){
            System.out.print(arr[i]+" ");
        }
    }
}
