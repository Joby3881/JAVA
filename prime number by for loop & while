public class Main {
     public static void main(String[] args) {

         for(int i=2;i<=100;i++){
            boolean isPrime=true;
            int divisior=2;
            while (divisior<=i/2){
                if(i%divisior==0){
                    isPrime=false;
                    break;
                }
                divisior++;
                }
             if (isPrime){
                 System.out.println(i);
            }
         }
     }
}
