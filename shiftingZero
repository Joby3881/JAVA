import java.util.Arrays;
public class Main {
    public static void main(String[] args) {
        int[] arr = {0, 1, 1, 0, 1, 0};
        int c=0;
        for (int i=0;i<arr.length;i++) {
            if (arr[i] != 0) {
                arr[c++] = arr[i];
            }
        }
               for (int j=c;j<arr.length;j++){
                   arr[j]=0;
            }
        System.out.println(Arrays.toString(arr));
        }
    }
